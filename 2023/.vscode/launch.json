{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "aoc debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "${fileDirname}\\build\\${fileBasenameNoExtension}_clang++.exe",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${fileDirname}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "gdb.exe",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "aoc debug"
      }
    ]
}